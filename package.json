{
	"name": "svelte-headless-form",
	"version": "0.2.1",
	"description": "A fast, light and batteries-included form library to make you more productive",
	"types": "dist/index.d.ts",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"scripts": {
		"build": "npm run clean && tsc -p ./tsconfig.build.json && vite build --config ./vite.config.ts && tsc -p ./tsconfig.build-types.json",
		"test": "vitest run --config ./vitest.config.ts",
		"test:coverage": "vitest run --config ./vitest.config.ts --coverage",
		"clean": "rm -rf dist"
	},
	"author": "Nicholas Trummer (nickt26)",
	"license": "MIT",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"keywords": [
		"svelte",
		"forms",
		"form",
		"validation",
		"form-validation",
		"svelte-forms",
		"svelte-form",
		"svelte-form-validation"
	],
	"type": "module",
	"devDependencies": {
		"@sveltejs/kit": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/svelte": "^5.1.0",
		"@testing-library/user-event": "^14.5.2",
		"@typescript-eslint/eslint-plugin": "^7.7.0",
		"@typescript-eslint/parser": "^7.7.0",
		"@vitest/coverage-v8": "^1.5.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"jsdom": "^24.0.0",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "^4.2.15",
		"svelte-check": "^3.6.9",
		"tslib": "^2.6.2",
		"typescript": "^5.5.2",
		"vite": "^5.2.10",
		"vitest": "^1.5.0",
		"yup": "^1.4.0",
		"zod": "^3.22.5"
	},
	"peerDependencies": {
		"svelte": ">=3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/nickt26/svelte-headless-form"
	}
}
